        app:layout_constraintTop_toBottomOf="@+id/texto2" />

    <TextView
        android:id="@+id/AdrianVillalba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="294dp"
